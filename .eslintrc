{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "hexo":"readonly",
    "CONFIG": "readonly",
    "Yun": "readonly"
  },
  "extends": "hexo",
  "rules": {
    "quotes": ["warn", "single"],
    "semi": ["warn", "always"],
    "strict": "off",
    "no-console": "off",
    "no-unexpected-multiline": "error",
    "no-unused-vars": "warn",
    "no-unused-expressions": "off"
  }
}
